{
    "private": true,
    "type": "module",
    "scripts": {
        "dev": "vite",
        "build": "vite build",
        "prepare": "husky",
        "prettier": "prettier --write .",
        "pint": "php vendor/bin/pint . --config=/pint.json",
        "analyse": "php ./vendor/bin/phpstan analyse --memory-limit=32G",
        "lint": "eslint resources/js --ext .js,.jsx .gitignore --fix",
        "all": "npm run prettier && npm run pint && npm run analyse"
    },
    "dependencies": {
        "@heroicons/react": "^2.2.0",
        "@xterm/xterm": "^5.5.0",
        "cors": "^2.8.5",
        "dotenv": "^16.4.7",
        "express": "^4.21.0",
        "framer-motion": "^12.4.7",
        "lodash": "^4.17.21",
        "socket.io": "^4.8.1",
        "socket.io-client": "^4.8.1",
        "ssh2": "^1.16.0"
    },
    "devDependencies": {
        "@eslint/js": "^9.21.0",
        "@headlessui/react": "^2.2.0",
        "@inertiajs/react": "^2.0.3",
        "@tailwindcss/forms": "^0.5.10",
        "@vitejs/plugin-react": "^4.3.4",
        "autoprefixer": "^10.4.20",
        "axios": "^1.7.9",
        "concurrently": "^9.1.2",
        "eslint": "^9.21.0",
        "eslint-config-prettier": "^10.0.1",
        "eslint-plugin-prettier": "^5.2.3",
        "eslint-plugin-react": "^7.37.4",
        "eslint-plugin-react-hooks": "^5.1.0",
        "globals": "^16.0.0",
        "husky": "^9.1.7",
        "laravel-vite-plugin": "^1.2.0",
        "lint-staged": "^15.4.3",
        "postcss": "^8.4.31",
        "prettier": "^3.4.2",
        "prettier-plugin-tailwindcss": "^0.6.11",
        "react": "^19.0",
        "react-dom": "^19.0",
        "tailwindcss": "^3.2.1",
        "vite": "^6.1.1"
    },
    "lint-staged": {
        "*.php": "php ./vendor/bin/pint",
        "*.{js,ts,vue}": "prettier --write"
    }
}
